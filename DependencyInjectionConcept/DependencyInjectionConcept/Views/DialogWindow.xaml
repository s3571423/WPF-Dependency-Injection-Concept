<Window x:Class="DependencyInjectionConcept.Views.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:DependencyInjectionConcept.Core.ViewModels;assembly=DependencyInjectionConcept.Core"
        d:DataContext="{d:DesignInstance viewModels:ViewModelBase, IsDesignTimeCreatable=False}"
        mc:Ignorable="d"
        Title="{Binding Title}" MinHeight="200" MinWidth="200" SizeToContent="WidthAndHeight">

    <Grid Margin="5">
        <ContentPresenter Content="{Binding}" Focusable="False"/>
    </Grid>
</Window>
